{
  "author": "Mario Reder <mreder1289@gmail.com>",
  "name": "ninres-example",
  "version": "1.0.0",
  "description": "Ninres example.",
  "repository": "https://github.com/Tarnadas/ninres-rs",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && yarn --cwd ../../.. build:web-dev && run-p serve watch",
    "serve": "webpack-dev-server -d --env=dev",
    "build": "yarn clean && yarn build:wasm && webpack --env=prod",
    "build:wasm": "yarn --cwd ../../.. build:web",
    "watch": "yarn --cwd ../../.. watch",
    "deploy": "gh-pages --dist dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rimraf ./dist && rimraf ../../../pkg"
  },
  "dependencies": {
    "@geist-ui/react": "^2",
    "react": "^17",
    "react-dom": "^17",
    "react-json-view": "^1"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-transform-react-jsx": "^7",
    "@babel/plugin-transform-typescript": "^7",
    "@babel/polyfill": "^7",
    "@babel/preset-env": "^7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "babel-loader": "^8",
    "core-js": "3",
    "eslint": "^7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ordered-imports": "^0",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "file-loader": "^6.2.0",
    "gh-pages": "^3",
    "html-webpack-plugin": "^5",
    "npm-run-all": "^4",
    "prettier": "^2",
    "ts-node": "^10",
    "typescript": "4.3",
    "webpack": "^5",
    "webpack-bundle-analyzer": "^4",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3"
  }
}
